class Solution {
public:
    int mySqrt(int x) {
        int low=1;
        int high=x;
        while(low<=high){
            long mid=low+(high-low)/2;
            long sqr=mid*mid;
            if(sqr==x){
                return mid;
            }
            else if(sqr<x){
                low=mid+1;

            }
            else{
                high=mid-1;
            }
        }
        return high;
        
    }
};
